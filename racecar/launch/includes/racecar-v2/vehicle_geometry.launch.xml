<!-- -*- mode: XML -*- -->
<launch>
  <arg name="racecar_version" />
  <arg name="include_static_transforms" default="true" />
  <arg name="vehicle_geometry_config" default="$(find racecar)/config/$(arg racecar_version)/vehicle_geometry.yaml" />

  <rosparam ns="/vehicle_geometry" file="$(arg vehicle_geometry_config)" command="load" />
  <include if="$(arg include_static_transforms)" file="$(find racecar)/launch/includes/$(arg racecar_version)/static_transforms.launch.xml" />
  
</launch>
